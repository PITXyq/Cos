-- Function to search for creatures in the value list
local function searchCreatures(searchText)
    local results = {}
    local searchTextLower = string.lower(searchText) 
    for creatureName, value in pairs(creatureValues) do
        local creatureNameLower = string.lower(creatureName)
        if creatureNameLower:find(searchTextLower, 1, true) then 
            table.insert(results, creatureName .. ": " .. value)
        end
    end
    return results
end

-- Textbox for searching creatures
CreatureListSection:AddTextbox({
    Name = "Search Creature",
    Default = "",
    TextDisappear = false,
    Callback = function(searchText)
        -- Clear previous results
        for _, label in ipairs(CreatureListSection:GetChildren()) do
            if label:IsA("TextLabel") then
                label:Destroy() 
            end
        end

        local searchResults = searchCreatures(searchText)
        if #searchResults == 0 then
            CreatureListSection:AddLabel("No creatures found.")
        else
            for _, result in ipairs(searchResults) do
                CreatureListSection:AddLabel(result)
            end
        end
    end  
})

-- Initially display all creatures (optional)
-- You can comment this out if you don't want to show all creatures initially
for creatureName, value in pairs(creatureValues) do
  CreatureListSection:AddLabel(creatureName .. ": " .. value)
end
